{
  "name": "erdia",
  "version": "2.2.1",
  "description": "CLI to generate mermaid.js ER diagram using TypeORM entity",
  "scripts": {
    "debug": "node -r ts-node/register -r tsconfig-paths/register --inspect-brk src/cli.ts",
    "prettier": "just --config ./.configs/just.config.ts prettier",
    "dev": "ts-node src/cli.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d ./examples/schema-type/dataSourceConfig.ts",
    "test": "jest --coverage --runInBand",
    "test-with-vm": "NODE_OPTIONS=--experimental-vm-modules npx jest --coverage --runInBand",
    "build": "just --config ./.configs/just.config.ts build",
    "clean": "just --config ./.configs/just.config.ts clean",
    "dts-bundle": "just --config ./.config/just.config.ts dts-bundle",
    "rollup:prod": "just --config ./.configs/just.config.ts rollup:prod",
    "rollup:dev": "just --config ./.configs/just.config.ts rollup:dev",
    "pub": "just --config ./.configs/just.config.ts pub",
    "unpub": "just --config ./.configs/just.config.ts unpub",
    "repub": "just --config ./.configs/just.config.ts repub",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "lint": "just --config ./.configs/just.config.ts lint",
    "just": "just --config ./.configs/just.config.ts",
    "httpd": "http-server -p 7879 dist/html -o /",
    "dd": "deps-diff md -d dev -d prod -d peer --title-list-type \"*\" --deps-list-type \"*\" --content-list-type \"*\"",
    "prepublishOnly": "node prepublish.js",
    "prepare": "husky install"
  },
  "keywords": [
    "typeorm",
    "entity",
    "mermaid",
    "mermaid.js",
    "mermaidjs",
    "er",
    "erdiagram",
    "er-diagram",
    "diagram",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/erdia.git"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./disg/cjs/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "typings": "./dist/esm/index.d.ts"
    }
  },
  "bin": {
    "erdia": "./dist/cli.js"
  },
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/erdia/issues"
  },
  "homepage": "https://github.com/imjuni/erdia#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@swc/core": "^1.3.69",
    "@swc/helpers": "^0.5.1",
    "@swc/jest": "^0.2.26",
    "@tsconfig/node16": "^16.1.0",
    "@types/crypto-js": "^4.1.1",
    "@types/ejs": "^3.1.2",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/jest": "^29.5.3",
    "@types/minimist": "^1.2.2",
    "@types/node": "^14.18.32",
    "@types/pouchdb": "^6.4.0",
    "@types/sanitize-html": "^2.9.0",
    "@types/source-map-support": "^0.5.4",
    "@types/yargs": "^17.0.24",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "better-sqlite3": "^8.4.0",
    "cross-env": "^7.0.3",
    "ctix": "^1.8.2",
    "deps-diff": "^1.0.2",
    "dts-bundle-generator": "^7.1.0",
    "eslint": "^8.45.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.4.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "execa": "^5.1.1",
    "get-tsconfig": "^4.2.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.6.1",
    "just-task": "^1.7.0",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "open": "^9.1.0",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.3",
    "rimraf": "^5.0.1",
    "rollup": "^3.26.3",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-swc3": "^0.9.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "alasql": "^4.1.3",
    "chalk": "^4.1.2",
    "consola": "^3.2.3",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.9",
    "del": "^6.1.1",
    "ejs": "^3.1.9",
    "fast-safe-stringify": "^2.1.1",
    "find-up": "^5.0.0",
    "fuse.js": "^6.6.2",
    "globby": "^11.1.0",
    "inquirer": "^8.2.4",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "jsonc-parser": "^3.2.0",
    "mathjs": "^11.8.2",
    "mermaid": "^10.3.1",
    "minimist": "^1.2.8",
    "my-easy-fp": "^0.21.0",
    "my-node-fp": "^0.8.1",
    "my-only-either": "^1.1.2",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.1",
    "prettier": "^3.0.1",
    "puppeteer": "^20.8.2",
    "read-pkg": "^5.2.0",
    "sanitize-html": "^2.11.0",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.1",
    "type-fest": "^4.2.0",
    "typeorm": "^0.3.17",
    "yargs": "^17.7.2"
  }
}
