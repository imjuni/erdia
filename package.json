{
  "name": "erdia",
  "version": "2.2.0",
  "description": "CLI to generate mermaid.js ER diagram using TypeORM entity",
  "scripts": {
    "debug": "node -r ts-node/register -r tsconfig-paths/register --inspect-brk src/cli.ts",
    "prettier": "just --config ./.configs/just.config.ts prettier",
    "dev": "ts-node src/cli.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "test": "jest --runInBand --coverage ",
    "build": "just --config ./.configs/just.config.ts build",
    "clean": "just --config ./.configs/just.config.ts clean",
    "dts-bundle": "just --config ./.config/just.config.ts dts-bundle",
    "rollup:prod": "just --config ./.configs/just.config.ts rollup:prod",
    "rollup:dev": "just --config ./.configs/just.config.ts rollup:dev",
    "pub": "just --config ./.configs/just.config.ts pub",
    "unpub": "just --config ./.configs/just.config.ts unpub",
    "repub": "just --config ./.configs/just.config.ts repub",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "lint": "just --config ./.configs/just.config.ts lint",
    "just": "just --config ./.configs/just.config.ts",
    "httpd": "http-server -p 7879 -o erdiagram.html",
    "dd": "deps-diff md -d dev -d prod -d peer --title-list-type \"*\" --deps-list-type \"*\" --content-list-type \"*\"",
    "prepublishOnly": "node prepublish.js",
    "prepare": "husky install"
  },
  "keywords": [
    "typeorm",
    "entity",
    "mermaid",
    "mermaid.js",
    "mermaidjs",
    "er",
    "erdiagram",
    "er-diagram",
    "diagram",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/erdia.git"
  },
  "main": "./dist/cjs/index.cjs",
  "module": "./disg/cjs/index.mjs",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "typings": "./dist/esm/index.d.ts"
    }
  },
  "bin": {
    "erdia": "./dist/cli.js"
  },
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/erdia/issues"
  },
  "homepage": "https://github.com/imjuni/erdia#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-terser": "^0.2.1",
    "@tsconfig/node14": "^1.0.3",
    "@types/crypto-js": "^4.1.1",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^14.18.32",
    "@types/sanitize-html": "^2.8.0",
    "@types/source-map-support": "^0.5.4",
    "@types/yargs": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "better-sqlite3": "^8.0.1",
    "cross-env": "^7.0.3",
    "ctix": "^1.2.2",
    "deps-diff": "^1.0.1",
    "dts-bundle-generator": "^7.1.0",
    "eslint": "^8.30.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "execa": "^5.1.1",
    "get-tsconfig": "^4.2.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "just-task": "^1.7.0",
    "open": "^8.4.0",
    "prettier-eslint": "^15.0.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "read-pkg": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.8.1",
    "rollup-plugin-ts": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "crypto-js": "^4.1.1",
    "del": "^6.1.1",
    "fast-safe-stringify": "^2.1.1",
    "find-up": "^5.0.0",
    "fuse.js": "^6.6.2",
    "globby": "^11.1.0",
    "inquirer": "^8.2.4",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "jsonc-parser": "^3.2.0",
    "mathjs": "^11.5.0",
    "mermaid": "^9.3.0",
    "my-easy-fp": "^0.18.0",
    "my-node-fp": "^0.8.1",
    "my-only-either": "^1.1.2",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1",
    "prettier": "^2.8.1",
    "puppeteer": "^19.4.1",
    "sanitize-html": "^2.8.1",
    "source-map-support": "^0.5.21",
    "tslib": "^2.4.1",
    "typeorm": "^0.3.11",
    "yargs": "^17.6.2"
  }
}
