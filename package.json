{
  "name": "erdia",
  "version": "2.1.1",
  "description": "CLI to generate mermaid.js ER diagram using TypeORM entity",
  "scripts": {
    "debug": "node -r ts-node/register -r tsconfig-paths/register --inspect-brk src/cli.ts",
    "dev": "ts-node src/cli.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "test": "just --config ./.configs/just.config.ts test",
    "build": "just --config ./.configs/just.config.ts build",
    "clean": "just --config ./.configs/just.config.ts clean",
    "dts-bundle": "just --config ./.config/just.config.ts dts-bundle",
    "webpack:prod": "just --config ./.configs/just.config.ts webpack:prod",
    "webpack:dev": "just --config ./.configs/just.config.ts webpack:dev",
    "pub": "just --config ./.configs/just.config.ts pub",
    "pub:prod": "just --config ./.configs/just.config.ts pub:prod",
    "lint": "just --config ./.configs/just.config.ts lint",
    "just": "just --config ./.configs/just.config.ts",
    "httpd": "http-server -p 19832 -o erdiagram.html"
  },
  "keywords": [
    "typeorm",
    "entity",
    "mermaid",
    "mermaid.js",
    "mermaidjs",
    "er",
    "erdiagram",
    "er-diagram",
    "diagram",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/erdia.git"
  },
  "main": "dist/erdia.js",
  "types": "dist/erdia.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "erdia": "./dist/cli.js"
  },
  "author": "ByungJoon Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/imjuni/erdia/issues"
  },
  "homepage": "https://github.com/imjuni/erdia#readme",
  "devDependencies": {
    "@jest/test-sequencer": "^28.1.1",
    "@tsconfig/node16": "^1.0.3",
    "@types/crypto-js": "^4.1.1",
    "@types/inquirer": "^8.2.1",
    "@types/inquirer-autocomplete-prompt": "^1.3.4",
    "@types/jest": "^28.1.4",
    "@types/node": "^18.0.1",
    "@types/sanitize-html": "^2.6.2",
    "@types/source-map-support": "^0.5.4",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "better-sqlite3": "^7.5.3",
    "cross-env": "^7.0.3",
    "ctix": "^1.2.2",
    "dts-bundle-generator": "^6.12.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.3",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "http-server": "^14.1.1",
    "jest": "^28.1.2",
    "just-scripts": "^2.0.0",
